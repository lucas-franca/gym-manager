{% extends "layout.njk" %}


{% block content %}
  <div class="card table-container">

  <a id="novo" href="/members/create">Novo</a>

    <table width="100%">
      <thead>
        <tr>
          <th>Membro</th>
          <th>Atendimento</th>
          <th>Ação</th>
        </tr>
      </thead>
      <tbody>
      {% for member in members %}
        <tr>
          <td>
            <span class="img" style="background-image: url({{member.avatar_url}})"></span>
          
            {{member.name}}
          </td>
          <td>{{instructor.services}}</td>
          <td>
            <a href="/members/{{member.id}}">ver</a>
          </td>
        </tr>
      {% endfor %}
      </tbody>
    </table>
  </div>

{% endblock content %}